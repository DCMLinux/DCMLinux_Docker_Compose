#Database Name
DCMLINUX_DBNAME=dcmlinux_manager
#Database Username
DCMLINUX_DBUSER=dcmlinux
#Database password
DCMLINUX_DBPASS=dcmlinux1
#Database Host Name (Supported external Databases)
DCMLINUX_DBHOST=db-dcmlinux

#DCM4CHEE Postgres Defaults for DCMLinux - adjust to your needs
POSTGRES_DB=pacsdb
POSTGRES_USER=dcmlinux
POSTGRES_PASSWORD=dcmlinux

#NODE
NODE_ENV=production

#DCM4CHEE LDAP Defaults for DCMLinux
